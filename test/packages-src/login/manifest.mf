{
  "pkgName": "bonjour",
  "version": "1.0.0",
  "cmds": [
    {
      "name": "bonjour",
      "type": "executable",
      "short": "print bonjour from command launcher",
      "executable": "{{.PackageDir}}/bonjour.{{if eq .Os \"windows\"}}bat{{else}}sh{{end}}",
      "args": [],
      "requiredFlags": [
        "name\t n\t greeting name",
        "language\t l\tgreeting language"
      ],
      "checkFlags": true,
      "requestedResources": [ "USERNAME", "PASSWORD" ]
    }
  ]
}
